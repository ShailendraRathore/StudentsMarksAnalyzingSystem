<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="448.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Login.StudentTableController">
    <children>
        <Label fx:id="label" layoutX="126" layoutY="120" minHeight="16" minWidth="69" />
        <TableView fx:id="tableUser" layoutX="13.0" layoutY="78.0" prefHeight="248.0" prefWidth="429.0">
            <columns>
                <TableColumn fx:id="columnRollno" prefWidth="59.0" text="ROLL NO" />
                <TableColumn fx:id="columnName" prefWidth="126.0" text="NAME" />
                <TableColumn fx:id="columnEnglish" prefWidth="56.0" text="ENGLISH" />
                <TableColumn fx:id="columnHindi" prefWidth="41.0" text="HINDI" />
                <TableColumn fx:id="columnMathematics" prefWidth="92.0" text="MATHEMATICS" />
                <TableColumn fx:id="columnScience" prefWidth="54.0" text="SCIENCE" />
            </columns>
        </TableView>
        <Label layoutX="150.0" layoutY="26.0" prefHeight="27.0" prefWidth="149.0" text="STUDENT DETAILS">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
      <Button fx:id="savebtn" layoutX="378.0" layoutY="352.0" mnemonicParsing="false" onAction="#saveData" text="Save" />
      <TextField fx:id="rollnoText" layoutX="13.0" layoutY="351.0" prefHeight="25.0" prefWidth="51.0" promptText="RollNo" />
      <TextField fx:id="nameText" layoutX="79.0" layoutY="351.0" promptText="Name" />
      <Button fx:id="addBtn" layoutX="234.0" layoutY="351.0" mnemonicParsing="false" onAction="#addBtnClicked" text="Add" />
      <Button fx:id="deleteBtn" layoutX="278.0" layoutY="352.0" mnemonicParsing="false" onAction="#deleteBtnClicked" text="Delete" />
      <Button fx:id="backBtn" layoutX="6.0" layoutY="14.0" mnemonicParsing="false" onAction="#backToLogin" text="Back" />

    </children>
</AnchorPane>
